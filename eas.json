{
  "cli": {
    "version": ">= 16.2.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_NO_DUPLICATE_LIBS": "true"
      },
      "android": {
        "buildType": "app-bundle"
      }
    }
  }, 
  "submit": {
    "production": {} 
  }
}